local lp = game.Players.LocalPlayer
local function getNPC()
	local dist, thing = math.huge
	for i,v in pairs(game:GetService("Workspace").Mobs.Bosses.Enmu:GetChildren()) do
		if v.Name == "Renpeke" then
			local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
			if mag < dist then 
				dist = mag 
				thing = v 
			end
		end
	end
	return thing
end

	local Rengoku = game:GetService("Workspace").Mobs.Bosses["Flame Trainee"].Renpeke


local PlayerABC = game.Players.LocalPlayer.Character.HumanoidRootPart
