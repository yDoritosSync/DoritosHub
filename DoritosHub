function AutoFarm()
    pcall(function()
        repeat task.wait()
            while _G.AutoFarm == true do task.wait()   
               local tween5 = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(16), {CFrame = CFrame.new(4562.95751953125, 725.3660888671875, -590.9823608398438)})
                tween5:Play()
                tween5.Completed:Wait(1) task.wait(2)
                for i,v in pairs(game:GetService("Workspace").Mobs.Bosses.Muichiro:GetChildren()) do
                if v.Name == "Muichiro Tokito" then
                    while _G.AutoFarm == true do task.wait()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,10,0)  * CFrame.Angles(math.rad(270), 0, 0)
                    if v.Humanoid.Health <= 0 then
                      local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(16), {CFrame = CFrame.new(1642.9918212890625, 299.98095703125, -329.4827575683594)})
                        tween:Play()
                        tween.Completed:Wait(1) task.wait(2)
                       for i,v2 in pairs(game:GetService("Workspace").Mobs.Bosses.Inosuke:GetChildren()) do 
                        if v2.Name == "Inosuke" then
                            while _G.AutoFarm == true do task.wait()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v2.HumanoidRootPart.CFrame * CFrame.new(0,10,0)  * CFrame.Angles(math.rad(270), 0, 0)
                        if v2.Humanoid.Health <= 0 then
                           local tween2 = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(16), {CFrame = CFrame.new(-1136.98193359375, 625.4895629882812, -632.75048828125)})
                           tween2:Play()
                           tween2.Completed:Wait(1) task.wait(2)
                         for i,v3 in pairs(game:GetService("Workspace").Mobs.Bosses["Flame Trainee"]:GetChildren()) do 
                            if v3.Name == "Renpeke" then
                                while _G.AutoFarm == true do task.wait()
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v3.HumanoidRootPart.CFrame * CFrame.new(0,10,0)  * CFrame.Angles(math.rad(270), 0, 0)    
                               if v3.Humanoid.Health <=0 then
                                local tween3 = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(16), {CFrame = CFrame.new(4395.2900390625, 675.1206665039062, -642.2671508789062)})
                                tween3:Play()
                                tween3.Completed:Wait(5)
                                    end
                                end
                            end
                        end
                    end
                end
            end
                 end
                       end
                    end
                end
            end
        end  
        until _G.AutoFarm == false or _G.AutoFarm ~= nil
    end)
end


function AutoFarm2()
    while _G.AutoFarm do task.wait()
        pcall(function()
            AutoFarm()
        end)
    end 
end 

function AutoLoot()
  repeat task.wait()
    while _G.AutoLoot == true do task.wait()
        local args = {
            [1] = "Health Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Stamina Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Jumbo Health Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Jumbo Health Regen Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Health Regen Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Jumbo Stamina Elixir"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Ore"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Inosuke Bottom"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Mist Katana"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Flame Katana"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Beast Katanas"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Insect Katana"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
        local args = {
            [1] = "Scythe"
        }
        
        workspace.Debree.Loot_Chest.Add_To_Inventory:InvokeServer(unpack(args))
    end
  until _G.AutoLoot == false
end


function AutoLoot2()
    while _G.AutoLoot do task.wait()
        pcall(function()
            AutoLoot()
        end)
end
end





local Tab = Window:MakeTab({
	Name = "AutoFarm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



Tab:AddToggle({
	Name = "AutoFarm Boss (Level)",
	Default = false,
	Callback = function(Value)
		_G.AutoFarm = Value
		AutoFarm2()
	end    
})
Tab:AddToggle({
	Name = "KillAura",
	Default = false,
	Callback = function(Value)
        _G.KillAura = Value
		KillAura()
	end    
})


Tab:AddToggle({
	Name = "AutoLoot!",
	Default = false,
	Callback = function(Value)
		_G.AutoLoot = Value
        AutoLoot2()
	end    
})


