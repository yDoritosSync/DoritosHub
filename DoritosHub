function Rotate()
	
 PlayerABC.CFrame = PlayerABC.CFrame * CFrame.Angles(1890,0,0)
 end
  

	 
	function AutoFarmBoss2()
		while _G.AutoFarmBoss == true do task.wait()
		pcall(function()
			moveto(getNPC().HumanoidRootPart.CFrame  + Vector3.new(0,10,0), 100)
		           if Rengoku.Humanoid.Health <= 10 or 20 or 30 then
					TeleportSafe()
				   end
			end) 
		   end
		end




function AutoFarmBoss4()
	while _G.AutoFarmBoss == true do task.wait()
	pcall(function()
		moveto(getNPC2().HumanoidRootPart.CFrame  + Vector3.new(0,10,0), 100)
	end)
end
end


