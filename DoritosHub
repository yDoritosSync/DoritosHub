--Locals





function TeleportSafe()
	local teleport_table = {
		location1 = Vector3.new(-1140.55151, 625.489563, -648.869873, -0.0198883228, -6.59708519e-08, 0.999802232, 4.15481658e-08, 1, 6.68103866e-08, -0.999802232, 4.28686953e-08, -0.0198883228), -- Primeiro Lugar
		location2 = Vector3.new() -- Segundo Lugar
	}

	local tween_s = game:GetService('TweenService')
	local tweeninfo = TweenInfo.new(29, Enum.EasingStyle.Linear) --Delay do Teleport

	local lp = game.Players.LocalPlayer

	function bypass_teleport(v)
		if lp.Character and
			lp.Character:FindFirstChild('HumanoidRootPart') then
			local cf = CFrame.new(v)
			local a = tween_s:Create(lp.Character.HumanoidRootPart, tweeninfo, { CFrame = cf })
			a:Play()

		end
	end

	bypass_teleport(teleport_table.location1)
end






function Rotate()
	
 PlayerRotate.CFrame = PlayerRotate.CFrame * CFrame.Angles(1890,0,0)
 end


 function Rotate4()
	while task.wait() do
		Rotate()
	end
 end
 function Rotate3()
	while task.wait() do
		if Rengoku.Humanoid.Health <= 3000 then
		   Rotate()
		end
		if Rengoku.Humanoid.Health == 10 then
		elseif Rengoku.Humanoid.Health == 10 then
		   if _G.Afb == true then
			   break
		   end
	end  
   end
end


	local lp = game.Players.LocalPlayer
	 function getNPC()
		local dist, thing = math.huge
		for i,v in pairs(game:GetService("Workspace").Mobs.Bosses["Flame Trainee"]:GetChildren()) do
			if v.Name == "Renpeke" then
				local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
				if mag < dist then 
					dist = mag 
					thing = v 
				end
			end
		end
		return thing
	end
	 
	local TweenService  = game:GetService("TweenService")
	local noclipE       = false
	local antifall      = false
	
	 
	local function noclip()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v:IsA("BasePart") and v.CanCollide == true then
				v.CanCollide = false
				game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,10,0)
			end
		end
	end
	 
	local function moveto(obj, speed)
		local info = TweenInfo.new(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - obj.Position).Magnitude)/ speed,Enum.EasingStyle.Linear)
		local tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, info, {CFrame = obj})
	 
		if not game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
			antifall = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart)
			antifall.Velocity = Vector3.new(0,0,0)
			noclipE = game:GetService("RunService").Stepped:Connect(noclip)
			tween:Play()
		end
	 
		tween.Completed:Connect(function()
			antifall:Destroy()
			noclipE:Disconnect()
		end)
	end

