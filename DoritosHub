	function TeleportToMuzan()
		local cframe = CFrame.new(game:GetService("Workspace").Muzan.SpawnPos.Value)
		local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,
			TweenInfo.new(20), { CFrame = cframe })
		tween:Play()
	end
	
	function RemoveSunDamage()
		repeat wait() until game:IsLoaded()
	
		local Sun_Damage = game:GetService("StarterPlayer").StarterPlayerScripts["Small_Scripts"].Gameplay["Sun_Damage"]
	
		Sun_Damage.Disabled = true
	
	end
	
	function KillAura2()
